                    if 220<=hand_keyPoints.landmark [8].x*CameraWindow.shape[1]<=350 and 540<=hand_keyPoints.landmark [8].y*CameraWindow.shape[0]<=580:
                        if cnt==2 and len (res.multi_hand_landmarks)==1:
                            cv2. rectangle (CameraWindow, (220,540),(350,580),( 0,255, 0), cv2.FILLED)
                            if 220<=hand_keyPoints.landmark[12].x*CameraWindow.shape[1]<=350 and 540<=hand_keyPoints.landmark[12].y*CameraWindow.shape[0]<=580:
                                button_cap.clicked_cap()
                                time.sleep(2)
                    if 370<=hand_keyPoints.landmark [8].x*CameraWindow.shape[1]<=500 and 540<=hand_keyPoints.landmark [8].y*CameraWindow.shape[0]<=580:
                        if cnt==2 and len (res.multi_hand_landmarks)==1:
                            cv2. rectangle (CameraWindow, (370,540),(500,580),( 0,255, 0), cv2.FILLED)
                            if 370<=hand_keyPoints.landmark[12].x*CameraWindow.shape[1]<=500 and 540<=hand_keyPoints.landmark[12].y*CameraWindow.shape[0]<=580:
                                button_spa.clicked_spa()
                                time.sleep(2)
                    if 520<=hand_keyPoints.landmark [8].x*CameraWindow.shape[1]<=650 and 540<=hand_keyPoints.landmark [8].y*CameraWindow.shape[0]<=580:
                        if cnt==2 and len (res.multi_hand_landmarks)==1:#Masters-IT Omkar_Kanse
                            cv2. rectangle (CameraWindow, (520,540),(650,580),( 0,255, 0), cv2.FILLED)
                            if 520<=hand_keyPoints.landmark[12].x*CameraWindow.shape[1]<=650 and 540<=hand_keyPoints.landmark[12].y*CameraWindow.shape[0]<=580:
                                button_bs.clicked_bs()
                                time.sleep(2)
                    if 670<=hand_keyPoints.landmark [8].x*CameraWindow.shape[1]<=800 and 540<=hand_keyPoints.landmark [8].y*CameraWindow.shape[0]<=580:
                        if cnt==2 and len (res.multi_hand_landmarks)==1:
                            cv2. rectangle (CameraWindow, (670,540),(800,580),( 0,255, 0), cv2.FILLED)
                            if 670<=hand_keyPoints.landmark[12].x*CameraWindow.shape[1]<=800 and 540<=hand_keyPoints.landmark[12].y*CameraWindow.shape[0]<=580:
                                button_en.clicked_en()#Masters-IT Omkar_Kanse
                                time.sleep(2)


                     #4Button up,down,left,right
                    if 980<=hand_keyPoints.landmark [8].x*CameraWindow.shape[1]<=1010 and 360<=hand_keyPoints.landmark [8].y*CameraWindow.shape[0]<=400:
                        if cnt==2 and len (res.multi_hand_landmarks)==1:
                            cv2. rectangle (CameraWindow, (980,360),(1010,400),( 0,255, 0), cv2.FILLED)
                            if 980<=hand_keyPoints.landmark[12].x*CameraWindow.shape[1]<=890 and 360<=hand_keyPoints.landmark[12].y*CameraWindow.shape[0]<=400:
                                button_up.clicked_up()
                                time.sleep(2)
                    if 980<=hand_keyPoints.landmark [8].x*CameraWindow.shape[1]<=1010 and 480<=hand_keyPoints.landmark [8].y*CameraWindow.shape[0]<=520:
                        if cnt==2 and len (res.multi_hand_landmarks)==1:
                            cv2. rectangle (CameraWindow, (850,480),(1010,520),( 0,255, 0), cv2.FILLED)
                            if 980<=hand_keyPoints.landmark[12].x*CameraWindow.shape[1]<=1010 and 480<=hand_keyPoints.landmark[12].y*CameraWindow.shape[0]<=520:
                                button_do.clicked_do()
                                time.sleep(2)#Masters-IT Omkar_Kanse
                    if 910<=hand_keyPoints.landmark [8].x*CameraWindow.shape[1]<=950 and 420<=hand_keyPoints.landmark [8].y*CameraWindow.shape[0]<=460:
                        if cnt==2 and len (res.multi_hand_landmarks)==1:
                            cv2. rectangle (CameraWindow, (910,420),(950,460),( 0,255, 0), cv2.FILLED)
