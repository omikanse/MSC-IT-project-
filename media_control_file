                                button_le.clicked_le()
                                time.sleep(2)
                    if 1050<=hand_keyPoints.landmark [8].x*CameraWindow.shape[1]<=1090 and 420<=hand_keyPoints.landmark [8].y*CameraWindow.shape[0]<=460:
                        if cnt==2 and len (res.multi_hand_landmarks)==1:
                            cv2. rectangle (CameraWindow, (1050,420),(1090,460),( 0,255, 0), cv2.FILLED)
                            if 1050<=hand_keyPoints.landmark[12].x*CameraWindow.shape[1]<=1090 and 420<=hand_keyPoints.landmark[12].y*CameraWindow.shape[0]<=460:
                                button_ri.clicked_ri()
                                time.sleep(2)#Masters-IT Omkar_Kanse
                
                        
                    elif cnt == 3 and len(res.multi_hand_landmarks)==1:#Masters-IT Omkar_Kanse
                        move_cursor(res.multi_hand_landmarks[0])
                        text = 'Mouse Cursor'
                        font = cv2.FONT_HERSHEY_SIMPLEX
                        cv2.putText(CameraWindow,text,(40,40),font,1,(255,255,255),2)
                    elif cnt == 4 and len(res.multi_hand_landmarks)==1:
                        pyautogui.click()
                        text = 'Mouse Click'
                        font = cv2.FONT_HERSHEY_SIMPLEX#Masters-IT Omkar_Kanse
                        cv2.putText(CameraWindow,text,(40,40),font,1,(255,255,255),2)

        #Masters-IT Omkar_Kanse
                        
        #Masters-IT Omkar_Kanse
                        
            
            cv2.imshow("OpenCamera",CameraWindow)
            

            if cv2.waitKey(1) == 27:
                cv2.destroyAllWindows()
                break
            
        


