            res = hand_obj.process(cv2.cvtColor(CameraWindow,cv2.COLOR_BGR2RGB))
            
            #Masters-IT Omkar_Kanse
            
            

            if res.multi_hand_landmarks:#Masters-IT Omkar_Kanse
                for hand_keyPoints in res.multi_hand_landmarks:
                    cnt = count_fingers(hand_keyPoints)

                    if cnt == 0:#Masters-IT Omkar_Kanse
                        continue
                    

