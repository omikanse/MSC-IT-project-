        def count_fingers(lst):
            cnt = 0
            thresh = (lst.landmark[0].y*100 - lst.landmark[9].y*100)/2

            if (lst.landmark[5].y*100 - lst.landmark[8].y*100) > thresh:
                cnt += 1
 
            if (lst.landmark[9].y*100 - lst.landmark[12].y*100) > thresh:
                cnt += 1
#created by Omkar_Kanse
            if (lst.landmark[13].y*100 - lst.landmark[16].y*100) > thresh:
                cnt += 1

            if (lst.landmark[17].y*100 - lst.landmark[20].y*100) > thresh:
                cnt += 1
#created by Omkar_Kanse
            if (lst.landmark[5].x*100 - lst.landmark[4].x*100) > 6:
                cnt += 1

            return cnt

        


        def move_cursor(hand_keyPoints):
            #def move_cursor(hand_keyPoints_left, hand_keyPoints_right):
            x= hand_keyPoints.landmark[4].x*2000
            y= hand_keyPoints.landmark[8].y*2000#created by Omkar_Kanse
            #x_left = hand_keyPoints_left.landmark[4].x * 640
            #y_left = hand_keyPoints_left.landmark[8].y * 480
#created by Omkar_Kanse
            #x_right = hand_keyPoints_right.landmark[4].x * 640
            #y_right = hand_keyPoints_right.landmark[8].y * 480
#created by Omkar_Kanse
            #x = (x_left + x_right) / 2
            #y = (y_left + y_right) / 2

            pyautogui.moveTo(x, y, duration=0)

